{
  "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "rules": {
    "indent":[
      2
    ],
    "keyword-spacing": "error",
    "linebreak-style": "error",
    "quotes": [
      "error",
      "single"
    ]
  }
}
