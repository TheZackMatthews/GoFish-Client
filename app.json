{
  "expo": {
    "name": "gofish-client",
    "slug": "gofish-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.thezackmatthews.gofishclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "The dual platform app to collect Skagit Fisheries Enhancement Group's salmon spawning data",
    "githubUrl": "https://github.com/TheZackMatthews/GoFish-client"
  }
}
